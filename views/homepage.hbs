<style>
	html {
  font-family: 'Poppins', sans-serif;
  font-size: 16px;
  line-height: 1.5;
  color: #fff;
  overflow-x: hidden;
  overflow-y: scroll;
  box-sizing: border-box;
  height: 100%;
  background-color: #232528;
}

html, body {
  -webkit-tap-highlight-color: transparent;
  -webkit-touch-callout: none;
  min-height: 100%;
  -webkit-text-size-adjust: 100%;
  -webkit-font-smoothing: antialiased;
}

.section {
  min-height: 100vh;
}

img {
  max-width: 100%;
}

h1, h2, h3, h4 {
  font-family: 'Noto Sans', cursive;
  font-weight: 600;
  letter-spacing: 1.5px;
}

h1 {
    font-size: 6.2em;
    line-height: 0.9;
    font-family: "Raleway", sans-serif;
    text-transform: uppercase;
    margin: 0;
    color: #cac9c9;
    padding: 24px 0;
    border-bottom: none;
    border-top: none;
}

.cat {
  font-family: 'Poppins', sans-serif;
  font-size: 14px;
  letter-spacing: 2px;
  position: relative;
  display: inline-block;
  text-transform: uppercase;
  color: #B1B3B2;
}
.cat.margin-bottom {
  margin-bottom: 1rem;
}
.cat:after {
  position: absolute;
  top: 50%;
  left: 100%;
  content: "";
  width: 50px;
  height: 1px;
  background-color: #B1B3B2;
  margin-left: 5px;
}

.contain {
  margin: 0 10%;
}

.hero-text {
  width: 80%;
  float: left;
}
@media (min-width: 600px) {
  .hero-text {
    width: 50%;
  }
}

.description {
  letter-spacing: 1px;
  width: 100%;
  float: left;
  margin-bottom: 1rem;
}
@media (min-width: 600px) {
  .description {
    width: 50%;
    margin-right: 50%;
  }
}
.description p {
  font-family: 'Playfair Display', serif;
}

.image-one {
  width: 80%;
  float: left;
  margin-right: 50%;
  margin-bottom: 50px;
}
@media (min-width: 600px) {
  .image-one {
    width: 50%;
  }
}

.image-two {
  width: 90%;
  float: left;
  margin-left: 10%;
  margin-bottom: 50px;
}
@media (min-width: 600px) {
  .image-two {
    width: 75%;
    margin-left: 25%;
  }
}

.hidden {
  position: absolute;
  top: 0;
  left: -9999999px;
}

#menu {
  z-index: 10;
  position: fixed;
  right: 0;
  top: 50%;
  transform: translateY(-50%);
  list-style: none;
  padding: 10px;
}
#menu li {
  position: relative;
  height: 30px;
  width: 30px;
  margin-bottom: 10px;
}
#menu li.active a {
  opacity: 1;
}
#menu li.active a:before {
  width: 30px;
  height: 30px;
}
#menu a, #menu a:visited {
  opacity: 0.5;
  transition: all 500ms ease-in-out;
}
#menu a:after, #menu a:visited:after {
  content: "";
  height: 10px;
  width: 10px;
  background-color: white;
  border-radius: 50%;
  position: absolute;
  top: 50%;
  left: 50%;
  transform: translate(-50%, -50%);
  z-index: 1;
}
#menu a:before, #menu a:visited:before {
  content: "";
  height: 5px;
  width: 5px;
  border: 1px solid white;
  border-radius: 50%;
  position: absolute;
  top: 50%;
  left: 50%;
  transform: translate(-50%, -50%);
  z-index: 0;
  transition: all 500ms ease-in-out;
}
#menu .active {
  color: red;
}

#bg {
  z-index: -1;
  position: fixed;
  top: 0%;
  left: 0%;
  width: 100%;
  height: 100%;
}
#bg img {
  position: absolute;
  top: 40%;
  left: 50%;
  transform: translate(-50%, -40%);
  max-width: 900px;
}

.inspiration-button {
  position: fixed;
  display: inline-block;
  z-index: 100;
  bottom: 1rem;
  right: 5%;
  color: white;
  text-decoration: none;
  padding: 0.75rem 1rem;
  border-radius: 80px;
  background: transparent;
  border: 1px solid white;
  opacity: 0.5;
  transition: all 500ms ease;
}
.inspiration-button:hover, .inspiration-button:focus, .inspiration-button:active {
  background-color: white;
  color: black;
  opacity: 1;
}
</style>
</head>
