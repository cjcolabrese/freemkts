<script>
  $(document).ready(function() {
    $("#fullpage").fullpage({
      anchors: ["firstPage", "secondPage", "thirdPage", "fourthPage"],
      menu: "#menu",
      autoScrolling: false
    });
  });

  // ScrollMagic: init controller
  var controller = new ScrollMagic.Controller();

  // Tween for background zoom - Starts on page scroll
  var scene = new ScrollMagic.Scene({
    triggerElement: "body",  // Trigger on body to start zooming immediately
    duration: $(window).height() * 2 // Duration for zooming effect
  })
  .setTween("#bg img", {
    scale: 1.5,  // Zoom the image
    top: "10%",  // Adjust top positioning during zoom
    left: "50%",
    opacity: 0.25
  })
  .addTo(controller);

</script>


